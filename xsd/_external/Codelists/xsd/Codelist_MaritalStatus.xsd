<xsd:schema targetNamespace="http://eurovoc.europa.eu/4184" 
xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" 
xmlns="http://eurovoc.europa.eu/4184"
xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">

	<xsd:simpleType name="MaritalStatusType">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="http://eurovoc.europa.eu/74">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>cohabitation</ccts:Name>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="http://eurovoc.europa.eu/2334">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>divorced person</ccts:Name>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="http://eurovoc.europa.eu/2335">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>married person</ccts:Name>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="http://eurovoc.europa.eu/6538">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>polygamy</ccts:Name>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="http://eurovoc.europa.eu/2339">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>seperated person</ccts:Name>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="http://eurovoc.europa.eu/2238">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>single parent</ccts:Name>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="http://eurovoc.europa.eu/5345">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>unmarried person</ccts:Name>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="http://eurovoc.europa.eu/2341">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>widowed person</ccts:Name>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
